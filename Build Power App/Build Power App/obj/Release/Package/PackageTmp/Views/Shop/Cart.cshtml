                <style>
                    .alert {
                        padding: 24px;
                        background-color: cadetblue;
                        color: white;
                        border: 3px solid white;
                    }

                    .closebtn {
                        margin-left: 15px;
                        color: white;
                        font-weight: bold;
                        float: right;
                        font-size: 22px;
                        line-height: 20px;
                        cursor: pointer;
                        transition: 0.3s;
                    }

                        .closebtn:hover {
                            color: black;
                        }
                </style>
                <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
                <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
                <script type="text/javascript">

                    $(document).ready(function () {
                        $(".addvalue").on('click', function () {

                            var quantityInput = $(this).parent().find('input');
                            var val = parseInt(quantityInput.val());
                            val = val + 1;
                            quantityInput.val(val);


                        });



                        $(".minvalue").on('click', function () {


                            var quantityInput = $(this).parent().find('input');
                            var val = parseInt(quantityInput.val());
                            val = val - 1;
                            if (val < 0) { val = 0;}
                            quantityInput.val(val);


                        });
                    });
                </script>
                <style>
                    .alert {
                        padding: 20px;
                        background-color: #f44336;
                        color: white;
                    }

                    .closebtn {
                        margin-left: 15px;
                        color: white;
                        font-weight: bold;
                        float: right;
                        font-size: 22px;
                        line-height: 20px;
                        cursor: pointer;
                        transition: 0.3s;
                    }

                        .closebtn:hover {
                            color: black;
                        }
                </style>
                @{
